{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["prettier/@typescript-eslint", "plugin:prettier/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn"
  }
}
